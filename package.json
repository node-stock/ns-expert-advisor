    "ns-findata": "0.0.15",
    "ns-manager": "0.0.3",
    "ns-strategies": "0.0.1",
    "ns-types": "0.0.3",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.2",
    "@types/config": "0.0.32",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.20",
    "@types/power-assert": "^1.4.29",
    "config": "^1.26.1",
    "debug": "^3.1.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.5.0",
    "power-assert": "^1.4.4",
